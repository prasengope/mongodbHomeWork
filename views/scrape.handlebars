<div id="container" style="margin-right: 5%;">
      <div class="row">
        <div class="col-md-6">

        </div>
        <div class="col-md-6">
          <h1>Scraped Articles</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-md-6">

        </div>
        <div class="col-md-6">
          <a href="/saved"><button>Saved Articles</button></a>
          <a href="/" id="homeLink"><button style="margin-left:2%;">Home</button></a>
        </div>
      </div>
      <div class="row">
        <div class="col-md-6">
          
        </div>
        <div class="col-md-6">
          <div id="articles" style="padding-right:3%; padding-top:2%">

            {{#each articles}}
              <hr>
                <button data-id={{@index}} class="save-article" style="padding: 5px 10px; float:right">Save</button>  
                <h4 id="title-{{@index}}"><strong>{{this.title}}</strong></h4>
                <a href="{{this.link}}" id="link-{{@index}}">{{this.link}}</a>
              <hr>
            {{/each}}
      
          </div>
        </div>
      </div>
    </div>
